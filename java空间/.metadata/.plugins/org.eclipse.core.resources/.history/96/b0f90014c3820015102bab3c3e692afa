package javacore;

interface Shape
{
	public abstract double square();  
}
class Circle implements Shape
{
	private double r;
	public Circle(double r)
	{
		this.r=r;
	}
	public double square()
	{
		return 2*r*r;
	}
	
}
class Triangle implements Shape
{
	private double d;
	private double h;
	public Triangle(double d,double h)
	{
		this.d=d;
		this.h=h;
	}
	
	
}
