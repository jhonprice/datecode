package bigdata;

public class test {

	public static void main(String[] args) {
		System.out.println(func2("6"));

	}
	public static boolean func2(String s)
	{
		if(s.equals("."))
			return true;
		if((s.indexOf("0123456789"))>=0)
			return true;
		if(s.equals("Ò¼")||s.equals("·¡"))
		{
			return true;
		}
		return false;
		
	}

}
