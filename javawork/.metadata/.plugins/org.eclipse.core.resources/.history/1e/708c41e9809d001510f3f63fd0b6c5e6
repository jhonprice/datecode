public class test5 {

	public static void main(String[] args) throws IOException{
		String s1=new String();
		try
		{
			int I=1;
			BufferedReader in=new BufferedReader(new FileReader("F://缓存区//in.csv"));
			s1=in.readLine();
			String no;
			while((s1=in.readLine())!=null)
			{
				String s2;
				s2=s1.substring(0);
				//System.out.println(s1+"\n");
				if(I==1)
				{
					no=new String(s1.substring(0,s1.indexOf(",")));
					//s1=s1.substring(s1.indexOf(",")+1);
