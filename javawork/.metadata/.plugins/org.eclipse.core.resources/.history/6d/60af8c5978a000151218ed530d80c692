package bigdata;

public class test {

	public static void main(String[] args) {
		System.out.println(func2("Ë®"));

	}
	public static boolean func2(String s)
	{
		if(s.equals(".")||s.equals("£¬"))
			return true;
		if(s.equals("0")||s.equals("1")||s.equals("2")||s.equals("3")||s.equals("4")||s.equals("5")||s.equals("6")||s.equals("7")||s.equals("8")||s.equals("9"))
			return true;
		if(s.equals("Áã")||s.equals("©–")||s.equals("O")||s.equals("o"))
			return true;
		if(s.equals("Ò»")||s.equals("¶þ")||s.equals("Èý")||s.equals("ËÄ")||s.equals("Îå")||s.equals("Áù")||s.equals("Æß")||s.equals("°Ë")||s.equals("¾Å")||s.equals("Ê®")||s.equals("°Ù")||s.equals("Ç§")||s.equals("Íò"))
			return true;
