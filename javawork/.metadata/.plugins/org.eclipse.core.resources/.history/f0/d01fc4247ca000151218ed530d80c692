package bigdata;
import java.util.*;
public class test {

	public static void main(String[] args)
	{
		
	}
	public static String func3(String s)
	{
		s.replace(" ", "");
		//////////////////////////////////////////
		s.replace("£¬",  "");
		s.replace("O",  "0");
		s.replace("o",  "0");
		s.replace("©–", "0");
		s.replace("Áã", "0");
		///////////////////////////////////////////
		s.replace("Ò»", "1");
		s.replace("¶þ", "2");
		s.replace("Èý", "3");
		s.replace("ËÄ", "4");
		s.replace("Îå", "5");
		s.replace("Áù", "6");
		s.replace("Æß", "7");
		s.replace("°Ë", "8");
		s.replace("¾Å", "9");
		///////////////////////////////////////////
		s.replace("Ò¼", "1");
		s.replace("·¡", "2");
		s.replace("Èþ", "3");
		s.replace("ËÁ", "4");
		s.replace("Îé", "5");
		s.replace("Â½", "6");
		s.replace("Æâ", "7");
		s.replace("°Æ", "8");
		s.replace("¾Á", "9");
		//////////////////////////////////////////
		if(s.endsWith("Íò"))
			s=s+"0000";
		if(s.endsWith("Ç§")||s.endsWith("Çª"))
			s=s+"000";
		if(s.endsWith("°Ù")||s.endsWith("°Û"))
			s=s+"00";
		if(s.endsWith("Ê®")||s.endsWith("Ê°"))
			s=s+"0";
		//////////////////////////////////////////
		s.replace("Íò", "");
		s.replace("Ç§", "");
		s.replace("Çª", "");
		s.replace("°Ù", "");
		s.replace("°Û", "");
		s.replace("Ê®", "");
		s.replace("Ê°", "");
		
		
		return s;
	}

}
