package bigdata;

import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.FileReader;
import java.io.FileWriter;
import java.io.IOException;

public class test5 {

	public static void main(String[] args) throws IOException{
		String s1=new String();
		int i=-50;
		char c;
		int flag=2;
		//String s2=new String();
		try
		{
			BufferedReader in=new BufferedReader(new FileReader("F://»º´æÇø//in.csv"));
			BufferedWriter out=new BufferedWriter(new FileWriter("F://»º´æÇø//out.txt"));
			while((s1=in.readLine())!=null&&i<2)
			{
				System.out.println(s1);
				out.write(s1);
				out.newLine();
				i++;
				if(s1.charAt(s1.length()-1)=='"')
				{
					flag++;
					break;
				}
			}
			out.flush();
			in.close();
			out.close();
		}
		catch(IOException ioe)
		{
			ioe.printStackTrace();
		}

	}

}
