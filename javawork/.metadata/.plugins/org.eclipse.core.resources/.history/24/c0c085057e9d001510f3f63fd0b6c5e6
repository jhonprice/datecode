package bigdata;

import java.util.*;

class Student
{
	private int a;
	private String b;
	public Student(int a,String b)
	{
		this.setA(a);
		this.setB(b);
	}
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public String getB() {
		return b;
	}
	public void setB(String b) {
		this.b = b;
	}
}
public class sada {
	
	public static void main(String[] args) {
		ArrayList s=new ArrayList();
		Student s1=new Student(1,"asd");
		Student s2=new Student(2,"asds");
		s.add(s1);
		s.add(s2);
		System.out.println(s.size());

	}

}
