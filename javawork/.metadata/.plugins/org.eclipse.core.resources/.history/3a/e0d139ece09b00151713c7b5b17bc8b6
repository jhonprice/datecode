package book;

public class GenericsClassExtendsDeTest<T extends Number> {
	public int sum(T t1,T t2)
	{
		return t1.intValue()+t2.intValue();
	}

	public static void main(String[] args) {
		GenericsClassExtendsDeTest<Integer> gcedt1=new GenericsClassExtendsDeTest<Integer>();
		System.out.println(gcedt1.sum(new Integer(2), new Integer(5)));

	}

}
